[global]
ioengine=libaio
sync=1
direct=1
invalidate=1
bs=4k
iodepth=1
rw=write
runtime=60

[sdb-ceph001]
stonewall
name=sdb-ceph001
filename=/dev/sdb

[sdc-ceph001]
stonewall
name=sdc-ceph001
filename=/dev/sdc

[sdd-ceph001]
stonewall
name=sdd-ceph001
filename=/dev/sdd

[sde-ceph001]
stonewall
name=sde-ceph001
filename=/dev/sde

[sdf-ceph001]
stonewall
name=sdf-ceph001
filename=/dev/sdf

[sdg-ceph001]
stonewall
name=sdg-ceph001
filename=/dev/sdg

